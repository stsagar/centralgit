pipeline{
    agent any
    environment{
    PATH= "/usr/share/maven/bin:$PATH"
    }
    stages{
        stage("Git Checkout"){
            steps{
                git credentialsId: 'github', url: 'https://github.com/stsagar/centralgit.git'
            }
        }
        stage("Build"){
            steps{
                sh "mvn clean package"
            }
        }
    }
}
