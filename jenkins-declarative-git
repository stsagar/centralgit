pipeline{
    agent any
    environment{
    PATH= "/usr/share/maven/:$PATH"
    }
    stages{
        stage("Git Checkout"){
            steps{
                git credentialsId: 'github', url: 'https://github.com/stsagar/centralgit.git'
            }
        }
        stage("Maven Build"){
        steps{
        sh "mvn clean package"
        }
        }
    }
}
